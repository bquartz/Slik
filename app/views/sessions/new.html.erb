<h1>Iniciar session</h1>
<%= form_tag sessions_path do %>
  <div class="fila">
    <%= label_tag :usuario %>
    <%= text_field_tag :username, params[:username] %>
  </div>
  <div class="fila">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div class="actions"><%= submit_tag "Log in" %></div>
<% end %>
